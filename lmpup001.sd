    Proc (":cli csbatch prsup600 %s%s%s%s",Str(I_TRANS_CODE,"99"),Str(SFPMASTER:POLICY_NO,"999999999"),Str(SFPMASTER:POL_YEAR,"9999"),Str(SFPMASTER:END_SEQUENCE,"9999"))/noconsole
}
