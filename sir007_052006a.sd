Procedure Definition

procedure bop
begin
Access bopendorse,
   Set bopendorse:POLICY_NO    = SFPPRINT1:POLICY_NO ,
       bopendorse:POL_YEAR     = SFPPRINT1:POL_YEAR ,
       bopendorse:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

Access bopgeneral,
   Set bopgeneral:POLICY_NO    = SFPPRINT1:POLICY_NO ,
       bopgeneral:POL_YEAR     = SFPPRINT1:POL_YEAR ,
       bopgeneral:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

While bopendorse:POLICY_NO    = sfpend:POLICY_NO And
      bopendorse:POL_YEAR     = sfpend:POL_YEAR And
      bopendorse:END_SEQUENCE = sfpend:END_SEQUENCE

Begin
  if bopendorse:code one of I_CODE then
    {
      If bopendorse:limit[1] <> 0 then
        {
          l_found = 1
        }
    }
  next bopendorse
End

Access bopendorse,
   Set bopendorse:POLICY_NO    = SFPPRINT1:POLICY_NO ,
       bopendorse:POL_YEAR     = SFPPRINT1:POL_YEAR ,
       bopendorse:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

If l_found = 1 then
  {
    do print_bop_limit
  }

Else
  {
    do print_default_limit
  }

end

Procedure cpp
Begin
  Access cppendorse,
     Set cppendorse:POLICY_NO    = SFPPRINT1:POLICY_NO ,
         cppendorse:POL_YEAR     = SFPPRINT1:POL_YEAR ,
         cppendorse:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

  Access cppgeneral,
     Set cppgeneral:POLICY_NO    = SFPPRINT1:POLICY_NO ,
         cppgeneral:POL_YEAR     = SFPPRINT1:POL_YEAR ,
         cppgeneral:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

  While cppendorse:POLICY_NO    = sfpend_alias:POLICY_NO And
        cppendorse:POL_YEAR     = sfpend_alias:POL_YEAR And
        cppendorse:END_SEQUENCE = sfpend_alias:END_SEQUENCE

  Begin
    if cppendorse:code one of I_CODE then
      {
        If cppendorse:limit[1] > 0 then
          {
            l_found = 1
          }
      }
    next cppendorse
  End

  Access cppendorse,
     Set cppendorse:POLICY_NO    = SFPPRINT1:POLICY_NO ,
         cppendorse:POL_YEAR     = SFPPRINT1:POL_YEAR ,
         cppendorse:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

  If l_found = 1 then
    {
      do print_cpp_limit
    }

  Else
    {
      do print_default_limit
    }
end

