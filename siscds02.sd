Procedure Definition

Procedure access_sfpend_alias
Begin
Access sfpend_alias,
   Set sfpend_alias:policy_NO = scpCONTRACTOR:policy_NO ,
       sfpend_alias:pol_year  = scpcontractor:pol_year,
       sfpend_alias:end_sequence = scpcontractor:end_sequence,
       sfpend_alias:PREM_NO = scpCONTRACTOR:PREM_NO,
       sfpend_alias:BUILD_NO = scpCONTRACTOR:BUILD_NO, Generic

Repeat begin
Exit  If  sfpend_alias:policy_NO <> scpCONTRACTOR:policy_NO Or
         sfpend_alias:pol_year  <> scpcontractor:pol_year or
         sfpend_alias:end_sequence <> scpcontractor:end_sequence or
         sfpend_alias:PREM_NO <> scpCONTRACTOR:PREM_NO Or
         sfpend_alias:BUILD_NO <> scpCONTRACTOR:BUILD_NO
--Begin
     If sfpend_alias:CODE = L_CODE Then
         L_ADD_ENDORSEMENT = "N"
        Next sfpend_alias
  End
End


End
