                      "<033>&a90h+0V","$",L_LETTER_GOTHIC_BOLD_10,
                      Str(impENDORSE:PREMIUM[8],"ZZZ,ZZZ,ZZZ"),L_ARIAL_9,
                      "<033>&a900h+0V","$",L_LETTER_GOTHIC_BOLD_10,
                      Str(impENDORSE:LIMIT[8],"ZZ,ZZZ,ZZZ"),L_ARIAL_9,
